use store2
CREATE TABLE Product (
    Product_ID int NOT NULL,
    Product_name varchar(255) NOT NULL,
    Product_catrgory varchar(255),
    Product_image varchar,
  material varchar(255),
  color varchar(255),
  price varchar(255),
  sale varchar(255),
  arrival_date varchar(255),
    PRIMARY KEY (Product_ID)
);
